        "POST": [
            {
                "path": "/post",
                "description": "Returns POST request data (form, JSON, or raw body).",
                "example_usage": "curl -X POST -d 'key=value' https://httpbin.org/post"
            },
            {
                "path": "/forms/post",
                "description": "Similar to /post, specifically for form-encoded data. (No difference on httpbin for simple forms).",
                "example_usage": "curl -X POST -d 'name=test&email=a@b.com' https://httpbin.org/forms/post"
            },
            {
                "path": "/anything",
                "description": "Returns request data for any method. Very flexible for testing various request types.",
